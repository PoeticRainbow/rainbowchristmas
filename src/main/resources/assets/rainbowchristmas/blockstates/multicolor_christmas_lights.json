{
  "multipart": [
    {
      "apply": {
        "model": "rainbowchristmas:block/multicolor_christmas_lights_hanging"
      }, 
      "when": {
        "north": false,
        "east": false,
        "south": false,
        "west": false
      }
    },
    {
      "apply": {
        "model": "rainbowchristmas:block/multicolor_christmas_lights",
        "y": 180
      }, 
      "when": {
        "north": true
      }
    },
    {
      "apply": {
        "model": "rainbowchristmas:block/multicolor_christmas_lights",
        "y": 270
      }, 
      "when": {
        "east": true
      }
    },
    {
      "apply": {
        "model": "rainbowchristmas:block/multicolor_christmas_lights"
      }, 
      "when": {
        "south": true
      }
    },
    {
      "apply": {
        "model": "rainbowchristmas:block/multicolor_christmas_lights",
        "y": 90
      }, 
      "when": {
        "west": true
      }
    }
  ]
}